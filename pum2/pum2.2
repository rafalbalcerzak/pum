   "metadata": {},
   "source": [
    "### Własna implementacja"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "3278465f",
   "metadata": {},
   "outputs": [],
   "source": [
    "#analityczna\n",
    "def my_ridge(XY, C, k): \n",
    "    return np.linalg.pinv(XY.T @ XY + k * np.identity(len(XY.T))) @ XY.T @ C\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "c65cc66b",
   "metadata": {},
   "outputs": [],
   "source": [
    "my_time = time.time()\n",
    "for i in range(time_multiplicator):\n",
    "    my_ridge_ans = my_ridge(XY_train, C_train, 1e-100)\n",
    "my_time = (time.time() - my_time)/time_multiplicator \n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "1c185bb3",
   "metadata": {},
   "outputs": [],
   "source": [
    "def my_prediction(theta, XY):\n",
    "    Y_pred = XY @ theta\n",
    "    mean = np.mean(Y_pred)\n",
    "    \n",
    "    ans = []\n",
    "    \n",
    "    for x in Y_pred:\n",
    "        if x >= mean:\n",
    "            ans.append(1.0)\n",
    "        else:\n",
    "            ans.append(0.0)\n",
    "    \n",
    "    return np.asarray(ans)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "b546d506",
   "metadata": {},
   "outputs": [],
   "source": [
    "my_pred = my_prediction(my_ridge_ans, XY_test)"
   ]
