   "metadata": {},
   "outputs": [],
   "source": [
    "#ustawienia\n",
    "time_multiplicator = 100\n",
    "seed = 244810\n",
    "samples = 2410"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "56bd504d",
   "metadata": {},
   "outputs": [],
   "source": [
    "XY,C  = datasets.make_classification(n_features=2,n_clusters_per_class=1,n_redundant=0,n_samples=samples, random_state=seed,  )"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "81f9af55",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.model_selection import train_test_split\n",
    "XY_train, XY_test, C_train, C_test = train_test_split(XY, C, test_size=0.2, random_state=0)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "a5471e62",
   "metadata": {},
   "outputs": [],
   "source": [
    "# C_train = np.where(C_train==0, -1, C_train) \n",
    "# C_test = np.where(C_test==0, -1, C_test) "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "44d25608",
   "metadata": {},
   "outputs": [],
   "source": [
    "def norm(x):\n",
    "    return (x - np.min(x)) / (np.max(x) - np.min(x))\n",
    "\n",
    "\n",
    "XY_train = norm(XY_train)\n",
    "XY_test = norm(XY_test)\n",
    "C_train = norm(C_train)\n",
    "C_test = norm(C_test)"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "33c18685",
   "metadata": {},
   "source": [
    "### sklearn"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "91091068",
   "metadata": {},
   "outputs": [],
   "source": [
    "#sklearn\n",
    "sk_time = time.time()\n",
    "for i in range(time_multiplicator):\n",
    "    sk_ridge_ans= linear_model.RidgeClassifier(alpha=2e-16).fit(XY_train,C_train)\n",
    "sk_time = (time.time() - sk_time)/time_multiplicator \n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "43a23076",
   "metadata": {},
   "outputs": [],
   "source": [
    "sk_pred = sk_ridge_ans.predict(XY_test)"
   ]
  },
